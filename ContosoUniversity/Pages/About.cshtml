@page
@model ContosoUniversity.Pages.AboutModel

@{
    ViewData["Title"] = "Статистика клиентов";
}

    <h2>Статистика клиентов</h2>

<table class="table">
    <tr>
        <th>
            Дата регистрации
        </th>
        
        <th>
            Колличество клиентов
        </th>
    </tr>

    @foreach (var item in Model.Student)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.EnrollmentDate)
    </td>
    
    <td>
        @item.StudentCount
    </td>
</tr>
    }
</table>